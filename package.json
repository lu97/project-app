{
  "name": "project-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.3.4",
    "bpmn-js": "^11.4.1",
    "bpmn-js-properties-panel": "^1.19.0",
    "camunda-bpmn-moddle": "^7.0.1",
    "cors": "^2.8.5",
    "diagram-js": "^11.9.1",
    "express": "^4.18.2",
    "gojs": "^2.3.2",
    "gojs-react": "^1.1.2",
    "react": "^18.2.0",
    "react-bpmn": "^0.2.0",
    "react-dom": "^18.2.0",
    "react-js-diagrams": "^3.1.3",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "react-scripts": "5.0.1",
    "react-sequence-diagram": "^0.0.12",
    "redux": "^4.2.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start_local": "react-scripts start public --host=0.0.0.0 --port=3000 --middleware=./disable-browser-cache.js  && react-snap",
    "build": "react-scripts build",
    "go": "open http://localhost:3000; npm run server",
    "e2e": "nightwatch --config nightwatch.js",
    "run-e2e": "concurrently -s first -k \"npm start\" \"sleep 15; npm run e2e\"",
    "test": "./node_modules/.bin/concurrently -k 'npm run server' 'npm run e2e'",
    "start_network": "npm run server"
  },
  "reactSnap": {
    "source": "dist",
    "inlineCss": true,
    "removeBlobs": true,
    "removeScriptTags": true,
    "removeStyleTags": true,
    "minifyHtml": {
      "collapseWhitespace": false,
      "removeComments": false
    },
    "destination": "dist",
    "include": [
      "/",
      "/404",
      "/500"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
